#!/usr/bin/env python

"""
	@author: Jean-Lou Dupont
"""

import os
import sys

this_version = "$version"

cwd     = os.getcwd()
id      = "$Id$"

print "LITM: cwd[%s] id[%s]" % (cwd, id)

try:
	command = sys.argv[1]
	version = sys.argv[2]
	
	print "LITM: preinst: command[%s] configured version[%s] version[%s]" % (command, version, this_version)
except:
	pass
	

sys.exit(0)